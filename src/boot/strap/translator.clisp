(DEFUN |getIntermediateLispFile| (|file| |options|)
  (PROG (|out|)
    (DECLARE (SPECIAL |$effectiveFaslType|))
    (RETURN
      (PROGN
        (SETQ |out| (NAMESTRING (|getOutputPathname| |options|)))
        (COND
          (|out| (CONCAT (|shoeRemoveStringIfNec| |$effectiveFaslType|
                             |out|)
