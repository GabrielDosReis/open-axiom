\begin{page}{Asp8ExampleCode}{Asp8 Example Code}
\begin{verbatim}
      SUBROUTINE OUTPUT(XSOL,Y,COUNT,M,N,RESULT,FORWRD)
      DOUBLE PRECISION Y(N),RESULT(M,N),XSOL
      INTEGER M,N,COUNT
      LOGICAL FORWRD
      DOUBLE PRECISION X02ALF,POINTS(8)
      EXTERNAL X02ALF
      INTEGER I
      POINTS(1)=1.0D0
      POINTS(2)=2.0D0
      POINTS(3)=3.0D0
      POINTS(4)=4.0D0
      POINTS(5)=5.0D0
      POINTS(6)=6.0D0
      POINTS(7)=7.0D0
      POINTS(8)=8.0D0
      COUNT=COUNT+1
      DO 25001 I=1,N
        RESULT(COUNT,I)=Y(I)
25001 CONTINUE
      IF(COUNT.EQ.M)THEN
        IF(FORWRD)THEN
          XSOL=X02ALF()
        ELSE
          XSOL=-X02ALF()
        ENDIF
      ELSE
        XSOL=POINTS(COUNT)
      ENDIF
      END
\end{verbatim}
\end{page}
