\begin{page}{Asp74ExampleCode}{Asp74 Example Code}
\begin{verbatim}
      SUBROUTINE BNDY(X,Y,A,B,C,IBND)
      DOUBLE PRECISION A,B,C,X,Y
      INTEGER IBND
      IF(IBND.EQ.0)THEN
        A=0.0D0
        B=1.0D0
        C=-1.0D0*DSIN(X)
      ELSEIF(IBND.EQ.1)THEN
        A=1.0D0
        B=0.0D0
        C=DSIN(X)*DSIN(Y)
      ELSEIF(IBND.EQ.2)THEN
        A=1.0D0
        B=0.0D0
        C=DSIN(X)*DSIN(Y)
      ELSEIF(IBND.EQ.3)THEN
        A=0.0D0
        B=1.0D0
        C=-1.0D0*DSIN(Y)
      ENDIF
      END
\end{verbatim}
