      SUBROUTINE COEFFN(P,Q,DQDL,X,ELAM,JINT)
      DOUBLE PRECISION ELAM,P,Q,X,DQDL
      INTEGER JINT
      P=1.0D0
      Q=((-1.0D0*X**3)+ELAM*X*X-2.0D0)/(X*X)
      DQDL=1.0D0
      RETURN
      END
\end{verbatim}
