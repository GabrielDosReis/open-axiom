        ((OR (ATOM |bfVar#6|) (PROGN (SETQ |i| (CAR |bfVar#6|)) NIL))
         (RETURN NIL))
        (T (SETF (GET |i| 'SHOEPRE) 'T)))
      (SETQ |bfVar#6| (CDR |bfVar#6|)))))

