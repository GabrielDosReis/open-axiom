      ((SETQ #1# (|assoc| (|devaluate| #0#) |HomogeneousAggregate;AL|))
       (CDR #1#))
      (T (SETQ |HomogeneousAggregate;AL|
               (|cons5| (CONS (|devaluate| #0#)
                              (SETQ #1# (|HomogeneousAggregate;| #0#)))
                        |HomogeneousAggregate;AL|))
         #1#)))) 

