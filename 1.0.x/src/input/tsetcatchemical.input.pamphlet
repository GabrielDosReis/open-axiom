<<*>>=
-----------------------------------------------------------------------------
--% Domains Definitions
-----------------------------------------------------------------------------

)clear all
ls : List Symbol := [x4,x3,x2,x1,t];
V := OVAR(ls);
R := Integer;
E := IndexedExponents V
P := NSMP(R, V);
LP := List(P);

-----------------------------------------------------------------------------
--% Initialisations
-----------------------------------------------------------------------------

x1: P := 'x1;
x2: P := 'x2;
x3: P := 'x3;
x4: P := 'x4;
t: P := 't;
	

p1 := 2 - 7 * x1 + x1 ** 2 * x2 + t * (x3 - x1) ;
p2 := 6 * x1 - x1 ** 2 * x2 + 10 * t * (x4 - x2) ;
p3 := 2 - 7 * x3 + x3 ** 2 * x4 + t * (x1 - x3) ;
p4 := 6 * x3 - x3 **2 * x4  + 1 - t * (x2 - x4) ;

lp := [p1,p2,p3,p4];


-----------------------------------------------------------------------------
--% Computations
-----------------------------------------------------------------------------

T := WUTSET(R,E,V,P)

medialSet(lp)$T

characteristicSet(lp)$T

characteristicSerie(lp)$T

zeroSetSplit(lp)$T


T := REGSET(R,E,V,P)

zeroSetSplit(lp)$T

zeroSetSplit(lp,false)$T

@
